{
  "name": "react-json-editor",
  "version": "2.6.0",
  "description": "A stylish, editor-like, modular, react component for viewing, editing, and debugging javascript object syntax!",
  "author": "andrew.redican.mejia@gmail.com",
  "homepage": "https://github.com/czack425/react-json-editor#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:dist && npm run test",
    "build:dist": "rimraf lib && npm run tsc && npm run build:commonjs",
    "build:commonjs": "cross-env NODE_ENV=production BABEL_ENV=modules:commonjs babel ./src --out-dir ./lib/ --ignore *.test.js",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prepare": "npm run build:dist",
    "prepublish": "npm run build:dist",
    "test": "npm run test:start && npm run test:clear",
    "test:start": "cross-env NODE_ENV=test BABEL_ENV=modules:commonjs jest --updateSnapshot",
    "test:clear": "del-cli test/__snapshots__",
    "test:watch": "cross-env NODE_ENV=test BABEL_ENV=modules:commonjs jest --watch",
    "test:coverage": "cross-env NODE_ENV=test BABEL_ENV=modules:commonjs jest --coverage",
    "test:release": "npm run build && npm pack .",
    "release": "npm run build && npm publish .",
    "tsc": "tsc"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "mitsuketa": "^1.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "all-contributors-cli": "^5.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-extensible-destructuring": "^4.3.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "cross-env": "^7.0.2",
    "del-cli": "^1.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.1.0",
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.3"
  },
  "peerDependencies": {
    "react": ">=16.2.0",
    "react-dom": ">=16.2.0"
  },
  "repository": "git@github.com:czack425/react-json-editor.git",
  "bugs": {
    "url": "https://github.com/czack425/react-json-editor/issues"
  },
  "keywords": [
    "react-json-editor",
    "ajrm",
    "react json editor",
    "json",
    "input box",
    "editor",
    "react-json",
    "json editor",
    "json debug",
    "json schema",
    "json parser",
    "json viewer",
    "json validator",
    "mitsuketajs.org",
    "mitsuketa",
    "javascript object viewer",
    "javascript object notation",
    "json validator",
    "javascript object syntax validator",
    "react",
    "component",
    "syntax editor",
    "nosql",
    "wysiwyg",
    "WYSIWYG",
    "what you see is what you get",
    "schema editor",
    "english",
    "german",
    "spanish",
    "hindi",
    "indonesian",
    "portuguese",
    "chinese",
    "french",
    "russian",
    "japanese",
    "tamil"
  ],
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/test/jest.transform.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "testURL": "http://localhost",
    "setupFiles": [
      "./test/jest.setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ]
}
